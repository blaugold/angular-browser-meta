{
  "name": "angular-browser-meta",
  "version": "0.0.4",
  "description": "Set meta tags and title from resolved router Data.",
  "keywords": [
    "meta",
    "angular2",
    "angular",
    "title"
  ],
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "typedoc": "typedoc",
    "predoc": "rimraf docs",
    "doc": "cd src && ../node_modules/.bin/typedoc --options ../typedoc.json --out ../docs ./",
    "upload-coverage": "coveralls < coverage/lcov.info",
    "karma": "karma start",
    "test": "COVERAGE=true npm run karma -- --single-run",
    "copy": "cpy package.json README.md release",
    "ngc": "ngc -p tsconfig.aot.json",
    "clean:pre": "rimraf release",
    "prebuild": "npm run clean:pre",
    "build": "npm run ngc",
    "postbuild": "npm run copy",
    "release": "standard-version"
  },
  "author": "Gabriel Terwesten <gabriel@terwesten.net>",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^2.4.4",
    "@angular/router": "^3.4.4",
    "rxjs": "^5.0.3"
  },
  "devDependencies": {
    "@angular/common": "^2.4.7",
    "@angular/compiler": "^2.4.7",
    "@angular/compiler-cli": "^2.4.7",
    "@angular/core": "^2.4.7",
    "@angular/platform-browser": "^2.4.7",
    "@angular/platform-browser-dynamic": "^2.4.7",
    "@angular/platform-server": "^2.4.7",
    "@angular/router": "^3.4.7",
    "@types/chai": "^3.4.35",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "awesome-typescript-loader": "^3.0.4",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "coveralls": "^2.11.16",
    "cpy-cli": "^1.0.1",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "rimraf": "^2.5.4",
    "rxjs": "^5.1.1",
    "source-map-loader": "^0.1.6",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "standard-version": "^4.0.0",
    "typedoc": "^0.5.7",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "zone.js": "^0.7.7"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blaugold/angular-browser-meta.git"
  },
  "bugs": {
    "url": "https://github.com/blaugold/angular-browser-meta/issues"
  },
  "homepage": "https://github.com/blaugold/angular-browser-meta#readme",
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
